---
import { getCollection } from "astro:content";
import BaseLayout from "@/layouts/BaseLayout.astro";
import Parallax from "@/layouts/Parallax.astro";
import Perspective from "@/layouts/Perspective.jsx";
import Form from "@/components/global/Form.astro";
import Tecnologias from "@/components/sections/Tecnologias.astro";
import Clientes from "@/components/sections/Clientes.astro";
import Cases from "@/components/sections/Cases.astro";
import Cta from "@/components/sections/Cta.astro";
import CtaCenter from "@/components/sections/CtaCenter.astro";
import Faq from "@/components/sections/Faq.astro";
import Footer from "@/layouts/Footer.jsx";
---

<BaseLayout>
  <Perspective client:load />
  <Tecnologias />
  <Cases />
  <Clientes />
  <Parallax>
    <CtaCenter />
    <Cta />
  </Parallax>
  <Form />
  <Faq />
  <Footer client:load />
</BaseLayout>
